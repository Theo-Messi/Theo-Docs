{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "groupName": "deps",
      "matchPackageNames": ["/.*/"]
    }
  ],
  "rangeStrategy": "bump",
  "semanticCommits": "enabled",
  "timezone": "Asia/Shanghai"
}
